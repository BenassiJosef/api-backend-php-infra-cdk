{
  "name": "stampede/backend",
  "description": "The Stampede Backend.",
  "license": "BSD-3-Clause",
  "type": "project",
  "keywords": [
    "slim-framework",
    "skeleton",
    "rest"
  ],
  "authors": [],
  "autoload": {
    "psr-4": {
      "App\\": "app/src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "StampedeTests\\": "tests"
    }
  },
  "config": {
    "optimize-autoloader": true
  },
  "require": {
    "php": ">=7.3.0",
    "slim/slim": "^3.0",
    "ext-soap": "*",
    "monolog/monolog": "^2.0",
    "bshaffer/oauth2-server-php": "^1.8",
    "bshaffer/oauth2-server-httpfoundation-bridge": "^1.1",
    "symfony/psr-http-message-bridge": "^0.2.0",
    "kreait/firebase-php": ">=4.1.7",
    "doctrine/orm": "~2.7.4",
    "stripe/stripe-php": "^4.2",
    "lku/doctrine-guid": "^1.0",
    "phpmailer/phpmailer": "~6.1",
    "slim/twig-view": "^2.1",
    "twig/twig": "~1.0",
    "predis/predis": "1.1.1",
    "akrabat/rka-ip-address-middleware": "^0.5.0",
    "php-curl-class/php-curl-class": "^7.1",
    "facebook/graph-sdk": "^5.6",
    "aws/aws-sdk-php": "^3.80",
    "mixpanel/mixpanel-php": "2.*",
    "beberlei/doctrineextensions": "^1.2",
    "ext-gd": "^7.0",
    "plivo/plivo-php": "^4.1.0",
    "twilio/sdk": ">=5.1.0",
    "chargebee/chargebee-php": ">=2, <3",
    "gocardless/gocardless-pro": "^1.1",
    "brianmcdo/image-palette": "dev-master",
    "league/oauth2-google": "^2.0",
    "tecnickcom/tcpdf": "^6.2",
    "voku/css-to-inline-styles": "^2.0",
    "php-ai/php-ml": "^0.5.0",
    "sinergi/browser-detector": "^6.1",
    "egulias/email-validator": "^2.1",
    "ksubileau/color-thief-php": "^1.3",
    "piwik/device-detector": "^3.10",
    "fzaninotto/faker": "^1.7",
    "php-amqplib/php-amqplib": "^2.7",
    "ext-json": "*",
    "nesbot/carbon": "^2.27",
    "ramsey/uuid": "^3.8",
    "pelago/emogrifier": "^3.1",
    "ramsey/uuid-doctrine": "^1.5",
    "athari/yalinqo": "^2.4",
    "ext-intl": "*",
    "endroid/qr-code": "^3.7",
    "ext-bcmath": "*",
    "tuupola/base62": "^2.0",
    "danielstjules/stringy": "^3.1",
    "moneyphp/money": "^3.3",
    "dflydev/fig-cookies": "^2.0",
    "griffinledingham/php-apple-signin": "^1.1",
    "rap2hpoutre/remove-stop-words": "^0.3.0",
    "ext-pdo": "*",
    "guzzlehttp/guzzle": "^6.5.4",
    "ext-dom": "*",
    "ext-libxml": "*",
    "lukasoppermann/http-status": "^2.0",
    "spatie/string": "^2.2",
    "ergebnis/http-method": "^2.1",
    "google/cloud-core": "^1.41",
    "chrisullyott/php-filesize": "^4.1",
    "symfony/console": "^5.3"
  },
  "require-dev": {
    "phpunit/phpunit": ">=6.3",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "scripts": {
    "start": "php -S localhost:8080 -t public",
    "test": "phpunit",
    "post-install-cmd": [
      "mkdir -p ./cache/proxies",
      "vendor/bin/doctrine orm:generate-proxies ./cache/proxies"
    ]
  }
}
